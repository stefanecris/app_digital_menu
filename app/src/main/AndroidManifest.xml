<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stefane.digitalmenu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_chefe"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".activity.OrderCanceledActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.OrderSentActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activity.OrderItemsActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activity.QuantityItemsActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>